	 local Tab1o = MakeTab({Name = "Join Game"})
	 local Tab2o = MakeTab({Name = "Main"})

     
-------TOGGLE 


Toggle = AddToggle(Tab2o, {
    Name = "Auto Rejoin Game",
    Default = false,
    Callback = function(state)
        AutoCollectRunning = state
        
        if AutoCollectRunning then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/TDDuym500/Duym500/refs/heads/main/Auto%20Rejoin%20Game"))()
        else
            getgenv().AutoCollectRunning = false
        end

        print("Auto Collect:", AutoCollectRunning and "B·∫≠t" or "T·∫Øt")
    end
})


local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Camera = game.Workspace.CurrentCamera

local AutoESP = false  -- üöÄ Tr·∫°ng th√°i b·∫≠t/t·∫Øt ESP
local ESPObjects = {}  -- üî¥ L∆∞u tr·ªØ ESP c·ªßa t·ª´ng ng∆∞·ªùi ch∆°i

-- üéØ H√†m t·∫°o ESP 2D xuy√™n t∆∞·ªùng
function CreateESP(Player)
    if Player == LocalPlayer then return end -- ‚ùå Kh√¥ng hi·ªÉn th·ªã ESP c·ªßa ch√≠nh m√¨nh

    local ESP = Drawing.new("Text")
    ESP.Size = 18
    ESP.Color = Color3.fromRGB(255, 0, 0) -- üî¥ M√†u ƒë·ªè c·ªë ƒë·ªãnh
    ESP.Outline = true
    ESP.Center = true
    ESP.Visible = false
    ESPObjects[Player] = ESP -- üåü L∆∞u ESP v√†o danh s√°ch

    local function UpdateESP()
        RunService.RenderStepped:Connect(function()
            if AutoESP and Player and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
                local HRP = Player.Character.HumanoidRootPart
                local ScreenPos, OnScreen = Camera:WorldToViewportPoint(HRP.Position)

                if OnScreen then
                    local Distance = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - HRP.Position).Magnitude)
                    ESP.Position = Vector2.new(ScreenPos.X, ScreenPos.Y - 10)
                    ESP.Text = "üë§ " .. Player.Name .. " | üìè " .. Distance .. "m"
                    ESP.Visible = true
                else
                    ESP.Visible = false
                end
            else
                ESP.Visible = false
            end
        end)
    end

    if Player.Character then
        UpdateESP()
    end
    Player.CharacterAdded:Connect(UpdateESP)
end

-- üöÄ Th√™m ESP cho t·∫•t c·∫£ ng∆∞·ªùi ch∆°i hi·ªán t·∫°i
for _, Player in pairs(Players:GetPlayers()) do
    CreateESP(Player)
end

-- üîÑ T·ª± ƒë·ªông th√™m ESP khi c√≥ ng∆∞·ªùi ch∆°i m·ªõi v√†o
Players.PlayerAdded:Connect(CreateESP)



-- üîò Toggle ESP b·∫±ng n√∫t
Toggle = AddToggle(Tab2o, {
    Name = "Esp Player",
    Default = false,
    Callback = function(state)
        AutoESP = state
        
        if not AutoESP then
            -- üõë ·∫®n to√†n b·ªô ESP khi t·∫Øt
            for _, ESP in pairs(ESPObjects) do
                ESP.Visible = false
            end
        end

        print("ESP Xuy√™n T∆∞·ªùng:", AutoESP and "B·∫¨T" or "T·∫ÆT")
    end
})

--------Toggle Warkspeed

local WalkSpeedEnabled = false
local DefaultWalkSpeed = nil -- L∆∞u WalkSpeed g·ªëc c·ªßa game

local Toggle2 = AddToggle(Tab1o, {
    Name = "walkspeed",
    Default = false,
    Callback = function(value)
        WalkSpeedEnabled = value
    end
})

game:GetService("RunService").RenderStepped:Connect(function()
    local player = game.Players.LocalPlayer
    local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")

    if humanoid then
        -- N·∫øu b·∫≠t toggle, gi·ªØ t·ªëc ƒë·ªô 325
        if WalkSpeedEnabled then
            if not DefaultWalkSpeed then
                DefaultWalkSpeed = humanoid.WalkSpeed -- Ch·ªâ l∆∞u gi√° tr·ªã m·∫∑c ƒë·ªãnh 1 l·∫ßn
            end
            humanoid.WalkSpeed = 325
        else
            -- N·∫øu t·∫Øt toggle, kh√¥i ph·ª•c t·ªëc ƒë·ªô m·∫∑c ƒë·ªãnh c·ªßa game
            if DefaultWalkSpeed then
                humanoid.WalkSpeed = DefaultWalkSpeed
            end
        end
    end
end)



